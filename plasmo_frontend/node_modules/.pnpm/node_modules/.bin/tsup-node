#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/charlesvandenbosch/Desktop/my-extension/my-extension/node_modules/.pnpm/tsup@7.2.0_@swc+core@1.11.9_@swc+helpers@0.5.15__postcss@8.5.3_typescript@5.2.2/node_modules/tsup/dist/node_modules:/Users/charlesvandenbosch/Desktop/my-extension/my-extension/node_modules/.pnpm/tsup@7.2.0_@swc+core@1.11.9_@swc+helpers@0.5.15__postcss@8.5.3_typescript@5.2.2/node_modules/tsup/node_modules:/Users/charlesvandenbosch/Desktop/my-extension/my-extension/node_modules/.pnpm/tsup@7.2.0_@swc+core@1.11.9_@swc+helpers@0.5.15__postcss@8.5.3_typescript@5.2.2/node_modules:/Users/charlesvandenbosch/Desktop/my-extension/my-extension/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/charlesvandenbosch/Desktop/my-extension/my-extension/node_modules/.pnpm/tsup@7.2.0_@swc+core@1.11.9_@swc+helpers@0.5.15__postcss@8.5.3_typescript@5.2.2/node_modules/tsup/dist/node_modules:/Users/charlesvandenbosch/Desktop/my-extension/my-extension/node_modules/.pnpm/tsup@7.2.0_@swc+core@1.11.9_@swc+helpers@0.5.15__postcss@8.5.3_typescript@5.2.2/node_modules/tsup/node_modules:/Users/charlesvandenbosch/Desktop/my-extension/my-extension/node_modules/.pnpm/tsup@7.2.0_@swc+core@1.11.9_@swc+helpers@0.5.15__postcss@8.5.3_typescript@5.2.2/node_modules:/Users/charlesvandenbosch/Desktop/my-extension/my-extension/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-node.js" "$@"
fi
